{
  "env": {
    "es2021": true,
    "node": true,
    "jest": true
  },
  "extends": "eslint:recommended",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "script"
  },
  "rules": {
    "quotes": ["error", "single"],
    "semi": ["error", "always"],
    "implicit-arrow-linebreak": "error",
    "no-unused-vars": "warn"
  }
}
